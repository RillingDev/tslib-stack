{
    "name": "eslib-stack",
    "version": "v9.0.0",
    "description": "A small & flexible stack for building modern ES6 libraries.",
    "main": "./dist/myLib.common.js",
    "module": "./dist/myLib.esm.js",
    "types": "./types/main.d.ts",
    "settings": {
        "input": "main.ts",
        "namespace": {
            "module": "myLib",
            "file": "myLib"
        },
        "external": []
    },
    "scripts": {
        "start": "npm-run-all lint build test docs",
        "build": "rollup -c ./tasks/rollup.config.js",
        "build:iife": "rollup -c ./tasks/rollup.config.iife.js",
        "build:iife-min": "rollup -c ./tasks/rollup.config.iife-min.js",
        "watch": "rollup -c ./tasks/rollup.config.js --watch",
        "dist": "npm-run-all --parallel build build:iife build:iife-min",
        "lint": "npm-run-all --parallel lint:ts lint:es",
        "lint:ts": "tslint ./src/**/*.ts",
        "lint:es": "eslint ./spec/**/*.js",
        "fix": "npm-run-all fix:ts fix:es fix:prettier",
        "fix:ts": "tslint ./src/**/*.ts --fix",
        "fix:es": "eslint ./spec/**/*.js --fix",
        "fix:prettier": "prettier ./src/**/* ./spec/**/* --write",
        "test": "jasmine",
        "docs": "jsdoc -c jsdoc.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/eslib-stack.git"
    },
    "keywords": [
        "stack",
        "es"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/eslib-stack/issues"
    },
    "homepage": "https://github.com/FelixRilling/eslib-stack#readme",
    "dependencies": {},
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.42",
        "@babel/plugin-external-helpers": "^7.0.0-beta.42",
        "@babel/preset-env": "^7.0.0-beta.42",
        "docdash": "git+https://github.com/clenemt/docdash.git",
        "eslint": "^4.19.1",
        "jasmine": "^3.1.0",
        "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
        "npm-run-all": "^4.1.2",
        "prettier": "^1.11.1",
        "rollup": "^0.57.1",
        "rollup-plugin-babel": "^4.0.0-beta.3",
        "rollup-plugin-json": "^2.3.0",
        "rollup-plugin-node-resolve": "^3.3.0",
        "rollup-plugin-typescript2": "^0.12.0",
        "rollup-plugin-uglify-es": "0.0.1",
        "tslint": "^5.9.1",
        "typescript": "^2.8.1",
        "uglify-es": "^3.3.10"
    }
}
